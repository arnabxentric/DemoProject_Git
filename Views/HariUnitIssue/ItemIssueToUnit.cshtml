                            <label class="black_txt_010">Delivery Truck No:<span style="color:Red;">*</span> </label>
                            @Html.TextBoxFor(model => model.DeliveryTruckNo, new { @class = "form-control", @placeholder = "Enter Truck No", @maxlength = "100" })
                            @Html.ValidationMessageFor(model => model.DeliveryTruckNo)
                        </div>
                        <div class="form-group">
                            <label class="black_txt_010">Delivery Man:<span style="color:Red;">*</span> </label>
                            @Html.TextBoxFor(model => model.DeliveryManName, new { @class = "form-control", @placeholder = "Enter Delivery Man Name", @maxlength = "100" })
                            @Html.ValidationMessageFor(model => model.DeliveryManName)
                        </div>
                        <div class="form-group">
                            <label class="black_txt_010">Contact No:<span style="color:Red;">*</span> </label>
