                    <th>PCS</th>
                    <th>Weight</th>
                    <th>Avg. Rate</th>
                    <th>Amount</th>
                    <th>Proffit/Loss</th>

                </tr>
            </thead>

            <tbody>
                @foreach (var item1 in Model)
                {

                    Slno = Slno + 1;
                <tr>
                    <td>@Slno</td>

                    <td align="center">
                        @item1.FarmerName
                    </td>
                    <td align="center">
                        @item1.FlockCode
                    </td>
                    <td align="center">
                        @item1.LotNo
                    </td>
                   

                    <td> @Html.DisplayFor(modelItem => item1.PlacementDate)</td>
                    <td> @Html.DisplayFor(modelItem => item1.Age)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.ChicksPlaced)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.Mortality)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.RemainingChicks)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.PricePerChick)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.TotalChicksCost)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.FeedIssued)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.CostOfFeed)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.CostOfMed)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.CostOfOverhead)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.GcPaid)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.TotalCost)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.ProductionCost)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.AvgWt)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.TotalWeight)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.SaleStatus)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.SoldPcs)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.TotalWeight)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.AvgRate)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.SalesAmount)</td>
                    <td style="text-align:right"> @Html.DisplayFor(modelItem => item1.ProffitLoss)</td>
         
                </tr>
                }
            </tbody>


        </table>

