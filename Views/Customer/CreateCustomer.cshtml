<script type="text/javascript" language="javascript">

    $(document).ready(function () {


        $("#Discount").val('0');
        $("#OpeningBal").val('0');
        $("#FD1").val('0');
        $("#FD2").val('0');
        $("#FD3").val('0');
        $("#FD4").val('0');


        //        $(".clickcheck").on("click", function () {
        //
        //            if (this.value === "0.0") {
        //                this.value = '';
        //            }
        //        });



        $('.iCheck-helper').click(function () {

            var parent = $(this).parent().get(0);
            var checkboxId = parent.getElementsByTagName('input')[0].id;




            if ($('input[name="chkpostal"]:checked').length > 0) {


                $("#Country").val($("#PoCountry").val());
                $("#StateRegion").val($("#PoStateRegion").val());
                $("#City").val($("#PoCity").val());
                $("#AddressName").val($("#PoAddressName").val());
                $("#PostalCode").val($("#PoPostalCode").val());
            }

            if ($('input[name="chkpostal"]:checked').length == 0) {
                $("#PostalCountryId").val("");
                $("#StateRegion").val("");
                $("#City").val("");
                $("#AddressName").val("");
                $("#PostalCode").val("");
            }



        });




    });

</script>



<script type="text/javascript">


    $(document).ready(function () {



        $("#groupID").change(function () {


            $.post("/LedgerMaster/GetSubGroupName", { type: $("#groupID").val() }, function (data) {
                var select = $("#subgrp");
                select.empty();


                $.each(data, function (index, itemData) {
                    select.append($('<option/>', {
                        value: itemData.Id,
                        text: itemData.Name
                    }));
                });
            });
        });



    });

</script>






<script type="text/javascript">


    $(document).ready(function () {



        $("#subgrp").change(function () {


            $.post("/Customer/GetLedgerName", { type: $("#subgrp").val() }, function (data) {
                var select = $("#DiscountLedger");
                select.empty();


                $.each(data, function (index, itemData) {
                    select.append($('<option/>', {
                        value: itemData.Id2,
                        text: itemData.Name
                    }));
                });
            });
        });



    });

</script>




<script type="text/javascript">


    $(document).ready(function () {



        $("#btnsubmit").click(function () {



            if ($("#Code").val() == "") {

                alert("Please Enter Customer Code");
                return false;

            }

            if ($("#Name").val() == "") {

                alert("Please Enter Customer Name");
                return false;

            }


            if ($("#Discount").val() == "") {

                alert("Please Enter Discount");
                return false;

            }
            if ($("#FD1").val() == "") {

                alert("Please Enter Further Discount1");
                return false;

            }
            if ($("#FD2").val() == "") {

                alert("Please Enter Further Discount2");
                return false;

            }
            if ($("#FD3").val() == "") {

                alert("Please Enter Further Discount3");
                return false;

            }
            if ($("#FD4").val() == "") {

                alert("Please Enter Further Discount4");
                return false;

            }




            if ($("#FD2").val() > 0 && $("#FD1").val() == 0) {

                alert("Please select Further discount1");
                return false;

            }



            if ($("#FD3").val() > 0 && $("#FD2").val() == 0) {

                alert("Please select Further discount2");
                return false;

            }


            if ($("#FD4").val() > 0 && $("#FD3").val() == 0) {

                alert("Please select Further discount3");
                return false;

            }


            if ($("#OpeningBal").val() == "") {

                alert("Please Opening Balance");
                return false;

            }




        });



        $("#Discount").change(function () {

            if ($("#Discount").val() == '') {
                $("#Discount").val(0.0);
            }
        });
        $("#FD1").change(function () {

            if ($("#FD1").val() == '') {
                $("#FD1").val(0.0);
            }
        });
        $("#FD2").change(function () {

            if ($("#FD2").val() == '') {
                $("#FD2").val(0.0);
            }
        });
        $("#FD3").change(function () {

            if ($("#FD3").val() == '') {
                $("#FD3").val(0.0);
            }
        });
        $("#FD4").change(function () {

            if ($("#FD4").val() == '') {
                $("#FD4").val(0.0);
            }
        });

    });

</script>








